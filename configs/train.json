{
    "restart": false,
    "log_dir": "logs",

    "data": {
        "input_dir": "intermediate_files/split_data",
        "y_column": "Main_cluster_name",
        "torch_sparsity_type": "dense",
        "train_cluster_size": 5000,
        "val_cluster_size": 1000,
        "test_cluster_size": 1000
    },
    "dataloader": {
        "batch_size": 256,
        "num_workers": 4,
        "shuffle_train": true,
        "shuffle_val": false
    },
    "model": {
        "name": "SelectorMLP",
        "selector_type": "std",
        "L1_size": 512,
        "L2_size": 128,
        "L3_size": 64,
        "leak_angle": 0.2,
        "reg_type": "L1",
        "batch_norm": true
    },
    "training": {
        "lr": 0.0003,
        "weight_decay": 0.3,
        "dropout": 0.3,
        "batch_norm": false,
        "noise_std": 0.2,
        "max_epochs": 50,
        "alpha": 0.05
    },
    "svm": {
        "name": "SVM",
        "loss": "hinge_squared",
        "reg_type": "L1"
    },
    "checkpoint": {
        "dir": "checkpoints",
        "restart_run": "last",
        "save_top_k": 3,
        "save_last": true
    }
} 